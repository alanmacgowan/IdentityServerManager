@model IdentityServerManager.UI.Models.ApiResourceViewModel
@{
    ViewData["Title"] = "Edit Api Resource";
}

<div class="row">
    <div class="col-md-12">
        <form asp-action="Edit" id="create" class="editForm">

            <input type="hidden" asp-for="Id" />

            @Html.Partial("_formFields", Model)

        </form>
    </div>
</div>

@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    <script src="~/js/Views/ApiResources/edit.js"></script>
}
