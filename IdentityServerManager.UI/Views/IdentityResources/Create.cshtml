@model IdentityServerManager.UI.Models.IdentityResourceViewModel

@{
    ViewData["Title"] = "Create  Identity Resource";
}

<div class="row">
    <div class="col-md-12">
        <form asp-action="Create" id="create" class="editForm">

            <input type="hidden" asp-for="Id" />

            @Html.Partial("_formFields", Model)

        </form>
    </div>
</div>


@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
    <script src="~/js/Views/IdentityResources/create.js"></script>
}
